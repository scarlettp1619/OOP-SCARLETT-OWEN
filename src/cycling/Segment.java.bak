package cycling;

public class Segment {
	
	SegmentType segmentType;
	double segmentLocation;
	double segmentLength;
	double avgGradient;
	
	public Segment(Double loc, SegmentType type, Double aGrad, Double length){
		segmentLocation = loc;
		segmentType = type;
		avgGradient = aGrad;
		segmentLength = length;
	}
}
